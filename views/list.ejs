<%- include("header") -%>
 
  <div class="mysec">
    <div>
      <img src="logo.png" alt="logo photo" class="myphotos" />
    </div>
    <div class="box1" id="heading">
      <h1>
        <%= listTitle %>
      </h1>
    </div>
    <div>
      <img src="pic.jpg" alt="logo photo" class="myphotos" />
    </div>
  </div>

  <div class="box2">

    <% newListItems.forEach(function(newItem){ %>
      <form action="/delete" method="post">
        <div class="item">
          <input type="checkbox" name="checkbox" value="<%= newItem._id %>" onChange="this.form.submit()" />
          <p>
            <%= newItem.name %>
          </p>

        </div>
        <input type="hidden" name="listName" value="<%= listTitle %>" />
      </form>

      <% }); %>
        <form class="item" action="/" method="post">
          <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
          <button type="submit" name="list" value="<%= listTitle%>">+</button>
        </form>
  </div>
  <%- include("footer") -%>